{
  "preset": "react-native",
  "testPathIgnorePatterns": ["/node_modules/", ".history", "/coverage/"],
  "cacheDirectory": "./.cache",
  "coveragePathIgnorePatterns": ["coverage"],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "transformIgnorePatterns": ["/node_modules/(?!react-native|react-clone-referenced-element|react-navigation)"]
}
